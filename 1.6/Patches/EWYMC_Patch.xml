<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<!-- Core - muted eating sounds -->

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Core</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/SoundDef[defName = "RawVegetable_Eat"]/subSounds/li/volumeRange</xpath>
					<value><volumeRange>0~0</volumeRange></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/SoundDef[defName = "RawMeat_Eat"]/subSounds/li/volumeRange</xpath>
					<value><volumeRange>0~0</volumeRange></value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/SoundDef[defName = "Meal_Eat"]/subSounds/li/volumeRange</xpath>
					<value><volumeRange>0~0</volumeRange></value>
				</li>
			</operations>
		</match>
	</Operation>

</Patch>